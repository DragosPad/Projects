@model AirlineCompany.Models.Form



@{
    ViewBag.Title = "Index";
}

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Form</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.DateForm)
        </div>
        <div class="editor-field">
          @Html.TextBoxFor(model => model.DateForm, new {id = "ReservationDate", @class="datetime", type = "datetime"})
            @Html.ValidationMessageFor(model => model.DateForm)
        </div>
 
        <div class="editor-label">
            @Html.LabelFor(model => model.Location)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Location)
            @Html.ValidationMessageFor(model => model.Location)
        </div>
   
           <p>
            <input type="submit" value="Complete" />
        </p>
         <p> @Html.ActionLink("ViewSeat", "ViewSeat")
             </p>
         <p> @Html.ActionLink("View Fligth", "StartLocation", "Form")
</p>
    </fieldset>
}

<script type="text/javascript">

    
    $(document).ready(function () {
        $('.datetime').datepicker({
            dateFormat: "dd/M/yy",
            changeMonth: true,
            changeYear: true,
            yearRange: "-60:+0"
        });

    });
</script> 
 
 
     